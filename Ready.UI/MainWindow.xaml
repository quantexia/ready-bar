<Window x:Class="Ready.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ready.UI"
        xmlns:controls="clr-namespace:Ready.UI.Controls"
        mc:Ignorable="d" Title="MainWindow" 
        Height="450" Width="200">
    <DockPanel LastChildFill="True">
        <StackPanel DockPanel.Dock="Bottom">
            <Button Content="Init" Width="50" Click="Button_Click"></Button>
            <Button Content="Go" Width="50" Click="Button_Click"></Button>
        </StackPanel>
        <controls:LauncherStackView x:Name="lsv"/>
    </DockPanel>
</Window>
